{
  "presets": [
    "env"
  ],
  "env": {
    "test": {
      "plugins": [
        "istanbul",
        ["transform-es2015-modules-commonjs", { "loose": true }]
      ]
    },
    "commonjs": {
      "plugins": [
        ["transform-es2015-modules-commonjs", { "loose": true }]
      ]
    },
    "rollup": {
      "plugins": [
        "external-helpers"
      ]
    }
  }
}